    "@types/jest": "^26.0.23",
    "@types/morgan": "^1.9.2",
    "@types/supertest": "^2.0.11",
    "@typescript-eslint/eslint-plugin": "^4.24.0",
    "@typescript-eslint/parser": "^4.24.0",
    "eslint": "^7.3.1",
    "eslint-plugin-import": "^2.22.0",
